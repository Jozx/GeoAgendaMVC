@model IEnumerable<GeoAgenda.Models.Vehiculo>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.marca.NombreMarca)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Kilometraje)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.latitudBase)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.longitudBase)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.marca.NombreMarca)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kilometraje)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.latitudBase)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.longitudBase)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdVehiculo }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdVehiculo }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdVehiculo })
        </td>
    </tr>
}

</table>
